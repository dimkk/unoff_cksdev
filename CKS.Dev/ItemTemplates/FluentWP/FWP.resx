  <data name="WebPartDialogScriptBlock" xml:space="preserve">
    <value>&lt;script type="text/javascript"&gt;

    //Dialog opening
    function OpenDialog(pageUrl, webPartId) {

        //Create client context. 
        var clientContext = new SP.ClientContext();

        var options = SP.UI.$create_DialogOptions();
        //Call the client context to get the relative web url to apend to the layouts page url
        options.url = "/_layouts/WebParts/$fileinputname$/Edit$fileinputname$.aspx?pageUrl=" + pageUrl + "&amp;webPartId=" + webPartId;
        options.width = 600;
        options.height = 200;
        options.dialogReturnValueCallback = Function.createDelegate(null, CloseCallback);
        SP.UI.ModalDialog.showModalDialog(options);
    }

    var messageId;

    // Dialog callback 
    function CloseCallback(result, message) {
        if (result == SP.UI.DialogResult.OK) {

            //Get id 
            messageId = SP.UI.Notify.addNotification(message, false);

            //This invokes a postback update on the ASP.Net UpdatePanel to update the rendering
            __doPostBack('udpWebPartBody', '');
        }
        if (result == SP.UI.DialogResult.cancel) {

            SP.UI.Notify.addNotification(message, false);
        }
    } 

&lt;/script&gt;</value>
