<?xml version="1.0" encoding="utf-8"?>
<webParts>
  <webPart xmlns="http://schemas.microsoft.com/WebPart/v3">
    <metaData>
      <type name="$rootnamespace$.$subnamespace$.$WrapperWebPartClassName$, $SharePoint.Project.AssemblyFullName$" />
      <importErrorMessage>$import_error_message$</importErrorMessage>
    </metaData>
    <data>
      <properties>
        <property name="Title" type="string">$WebPartTitle$</property>
        <property name="Description" type="string">$WebPartDescription$</property>
      </properties>
    </data>
  </webPart>
</webParts>
