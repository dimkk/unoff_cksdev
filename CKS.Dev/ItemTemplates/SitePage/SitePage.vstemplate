<VSTemplate Version="2.0.0" 
            Type="Item" 
            xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:vssdk="http://schemas.microsoft.com/developer/vssdktemplate/2007"
            xsi:schemaLocation="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>Basic Site Page (CKSDev)</Name>
    <Description>A Site Page with default Web Part zones.</Description>
    <Icon>SitePage.ico</Icon>
    <FrameworkVersion>3.5</FrameworkVersion>
    <ProjectType>CSharp</ProjectType>
    <SortOrder>10</SortOrder>
    <DefaultName>BasicSitePage</DefaultName>
    <TemplateGroupID>SharePointItemTemplateGroupID</TemplateGroupID>
    <ShowByDefault>false</ShowByDefault>
    <vssdk:OutputSubPath>SharePoint\SharePoint14</vssdk:OutputSubPath>
  </TemplateData>
  <TemplateContent>
    <References>
      <Reference>
        <Assembly>Microsoft.SharePoint</Assembly>
      </Reference>
      <Reference>
        <Assembly>System</Assembly>
      </Reference>
      <Reference>
        <Assembly>System.Web</Assembly>
      </Reference>
      <Reference>
        <Assembly>System.Web.Extensions</Assembly>
      </Reference>
    </References>
    <ProjectItem ReplaceParameters="true" TargetFileName="$fileinputname$.aspx">Default.aspx</ProjectItem>
  </TemplateContent>
</VSTemplate>
